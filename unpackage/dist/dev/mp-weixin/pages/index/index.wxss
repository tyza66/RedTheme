
.top {
	height: 25px;
	width: 100%;
	background-color: #FFFFFF;
}
.tp{
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 100rpx;
	box-shadow: 0px 2px 5px #888888;
	background: #FFFFFF;
	z-index: 999;
}
.left{
	float: left;
	width: 100rpx;
	height: 100rpx;
	margin-left: 20rpx;
}
.right{
	float: right;
	height: 100rpx;
	width: 300rpx;
	display: flex;
	flex-direction: row;
	justify-content: center;
	align-items: center;
}
.xiao{
	margin: 0 auto;
	display: flex;
	flex-direction: row;
	justify-content: center;
	align-items: center;
}
.xiao-1{
	margin: 0 auto;
	display: flex;
	flex-direction: row;
	justify-content: center;
	align-items: center;
	border-bottom: #000000 solid 1rpx;
}
.xiao ._p,.xiao-1 ._p{
	color: #000000;
	font-size: 40rpx;
}
.xiao image,.xiao-1 image{
	width: 45rpx;
	height: 45rpx;
}
.a{
	width: 100%;
}
.have{
	width: 100%;
	height: 100rpx;
}
.shipin,.tuwen{
