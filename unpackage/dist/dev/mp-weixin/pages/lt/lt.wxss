
.top {
	height: 25px;
	width: 100%;
	background-color: #FFFFFF;
}
.items{
	width: 100%;
	height: 180rpx;
	display: flex;
	flex-direction: row;
	justify-content: center;
	align-items: center;
}
.item{
	width: 180rpx;
	height: 180rpx;
	margin: 0 auto;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
}
.item image{
	width: 100rpx;
	height: 100rpx;
}
.iti{
	width: 100%;
	height: 80rpx;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
}
.am {
		position: relative;
		width: 100%;
		height: 130rpx;
		background-color: #D0E0F3;
}
.am1 {
		position: relative;
		width: 100%;
		height: 130rpx;
		background-color: #FCE1C4;
}
.am .touxiang,
	.am1 .touxiang {
		position: absolute;
		width: 80rpx;
		height: 80rpx;
